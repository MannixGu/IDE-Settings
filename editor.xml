<application>
  <component name="CodeInsightSettings">
    <option name="PARAMETER_INFO_DELAY" value="0" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE:PREVIEW" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="c0534d" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern pattern="\bmtest\b.*" />
    <pattern pattern="\bmnote\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\bimpl\b.*">
      <option name="FOREGROUND" value="2ec03f" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>